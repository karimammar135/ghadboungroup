{
  "author": "TwicPics <hello@twic.pics> (https://www.twicpics.com)",
  "bugs": {
    "url": "https://github.com/twicpics/components/issues"
  },
  "description": "A Web component library that brings the power of TwicPics to your favorite web framework.",
  "homepage": "https://github.com/twicpics/components",
  "keywords": [
    "angular",
    "component library",
    "gatsby js",
    "next js",
    "nuxt js",
    "react",
    "responsive image",
    "svelte",
    "sveltekit",
    "twicpics",
    "ui library",
    "vue js"
  ],
  "license": "MIT",
  "name": "@twicpics/components",
  "repository": {
    "type": "git",
    "url": "git://github.com/twicpics/components.git"
  },
  "version": "0.29.0",
  "exports": {
    "./style.css": "./style.css",
    "./react": {
      "require": "./react/index.js",
      "import": "./react/module.mjs",
      "types": "./react/index.d.ts"
    },
    "./react-native": {
      "require": "./react-native/index.js",
      "import": "./react-native/module.mjs",
      "types": "./react-native/index.d.ts"
    },
    "./svelte3": {
      "require": "./svelte3/index.js",
      "import": "./svelte3/module.mjs",
      "types": "./svelte3/index.d.ts"
    },
    "./svelte4": {
      "require": "./svelte4/index.js",
      "import": "./svelte4/module.mjs",
      "types": "./svelte4/index.d.ts"
    },
    "./vue2": {
      "require": "./vue2/index.js",
      "import": "./vue2/module.mjs",
      "types": "./vue2/index.d.ts"
    },
    "./vue3": {
      "require": "./vue3/index.js",
      "import": "./vue3/module.mjs",
      "types": "./vue3/index.d.ts"
    },
    "./next": {
      "require": "./next/index.js",
      "import": "./next/module.mjs",
      "types": "./next/index.d.ts"
    },
    "./nuxt2": {
      "require": "./nuxt2/index.js"
    },
    "./nuxt3": {
      "require": "./nuxt3/index.js",
      "import": "./nuxt3/module.mjs"
    },
    "./gatsby/package.json": "./gatsby/package.json",
    "./webcomponents": {
      "require": "./webcomponents/index.js",
      "import": "./webcomponents/module.mjs",
      "types": "./webcomponents/index.d.ts"
    },
    "./angular11": "./angular11",
    "./angular12": "./angular12",
    "./angular13": "./angular13",
    "./angular14": "./angular14",
    "./angular15": "./angular15",
    "./angular16": "./angular16",
    "./angular17": {
      "main": "./angular12/main.js",
      "module": "./angular12/module.js",
      "es2015": "./angular12/module.js",
      "fesm2015": "./angular12/module.js",
      "typings": "./angular12/lib.d.ts"
    },
    "./svelte5": {
      "main": "./sveltekit/index.js",
      "module": "./sveltekit/index.js",
      "svelte": "./sveltekit/index.js",
      "types": "./sveltekit/index.d.ts"
    },
    "./sveltekit": {
      "main": "./sveltekit/index.js",
      "module": "./sveltekit/index.js",
      "svelte": "./sveltekit/index.js",
      "types": "./sveltekit/index.d.ts"
    },
    "./sveltekit/": "./sveltekit/"
  }
}